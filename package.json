{
  "name": "pangaea",
  "version": "1.0.0",
  "description": "A distributed publisher-subscriber App built with PostgreSQL and nodejs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "nodemon index.js",
    "start": "node index.js",
    "migrate": "sequelize db:migrate",
    "migrate:create": "sequelize migration:create --name",
    "migrate:undo": "sequelize db:migrate:undo",
    "model:generate": "npx sequelize-cli model:generate --name Subscriber --attributes url:string,topicId:string"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AugustineUmeagudosi/pangaea.git"
  },
  "keywords": [
    "Distributed",
    "App",
    "NodeJs",
    "PostgreSQL",
    "Express",
    "Subscriber",
    "Publisher"
  ],
  "author": "Augustine Umeagudosi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AugustineUmeagudosi/pangaea/issues"
  },
  "homepage": "https://github.com/AugustineUmeagudosi/pangaea#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.5.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2",
    "uuid": "^8.3.2"
  }
}
